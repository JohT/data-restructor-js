{
  "name": "data-restructor",
  "version": "2.3.0",
  "description": "Transforms parsed JSON objects into a uniform data structure",
  "main": "./src/js/datarestructor.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "test": "jasmine --config=./test/js/jasmine.json",
    "coverage": "nyc npm run test",
    "doc": "jsdoc -d docs --configure ./docs/jsdoc.json ./src/js/datarestructor.js",
    "dev": "parcel ./src/js/datarestructor.js",
    "build": "parcel build ./src/js/datarestructor.js ./src/js/datarestructor-ie.js ./src/js/templateResolver.js",
    "package": "npm run build && (npm run coverage & npm run doc)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JohT/data-restructor-js.git"
  },
  "keywords": [
    "restruct",
    "json",
    "data"
  ],
  "author": "JohT",
  "license": "SEE LICENCE IN LICENCE.txt",
  "bugs": {
    "url": "https://github.com/JohT/data-restructor-js/issues"
  },
  "homepage": "https://github.com/JohT/data-restructor-js#readme",
  "devDependencies": {
    "jasmine": "^3.6.4",
    "jsdoc": "^3.6.6",
    "nyc": "^15.1.0",
    "parcel-bundler": "^1.12.4"
  }
}
